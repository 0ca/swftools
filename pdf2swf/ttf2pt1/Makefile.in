top_builddir = ../..
srcdir = @srcdir@
top_srcdir = @top_srcdir@
include ../../Makefile.common
@SET_MAKE@

all: libpdf.a

%.o: %.c
	$(C) -I./ -Wno-parentheses $< -o $@

ttf2pt1_objects =  ft.o ttf.o pt1.o ttf2pt1.o t1asm.o

libpdf.a: $(ttf2pt1_objects)
	$(AR) r ttf2pt1.a $(ttf2pt1_objects)
	$(RANLIB) ttf2pt1.a

install:
uninstall:

clean: 
	rm -f *.o *.lo *.a *.la

